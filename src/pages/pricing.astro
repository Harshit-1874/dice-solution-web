---
import MainLayout from "../layouts/MainLayout.astro";
import Form from "../components/ui/Form.astro";
import priceData from "../data/priceDats.json";
import Card from "../components/ui/Card.astro";
---

<MainLayout title="Our Pricing">
    <main class="space-y-20">
        <div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 lg:px-3">
            <div class="mx-auto max-w-screen-md text-center mb-8 lg:mb-12">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900">Development Solutions for Every Business</h2>
                <p class="mb-5 font-light text-gray-500 sm:text-xl">At Dice Solutions, we offer scalable development packages that combine innovation, technical expertise, and business value to help you achieve your digital goals.</p>
            </div>
            <Form />
        </div>
    </main>
</MainLayout>